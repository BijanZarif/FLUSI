#!/bin/bash

VDFNAM="flusi.vdf"
vdfcreate -dimension 64x64x64 -numts 49 -level 10 -varnames ux:uy:uz:vorx:vory:vorz:vorabs:p ${VDFNAM}


for PRE in {'ux','uy','uz','p','vorx','vory','vorz','vorabs'}
do
    VNAM=${PRE}
    FLIST=$( ls ${VNAM}_*.binary )
    N=0
    for FICH in ${FLIST}
    do
	echo ${N}
	echo ${FICH}
	raw2vdf -ts ${N} -varname ${VNAM} ${VDFNAM} ${FICH} 
	N=$((N+1))
    done
done

